v>\n    <sui-container text>\n      <sui-grid-row>\n      <sui-grid-column :width=\"9\">\n\n              <sui-grid-column class=\"arrows\" :width=\"3\">\n                 <router-link to=\"/student/projects\"><h4><i class=\"angle double left icon\"></i>Back</h4></router-link>\n              </sui-grid-column>\n\n              <sui-segment raised>\n\n                <h1 class=\"highlight\">\n                  <i v-if=\"getCurrentProject.role_type && getCurrentProject.role_type.includes('Data')\" class=\"flask icon\"></i>\n                  <i v-if=\"getCurrentProject.role_type && getCurrentProject.role_type.includes('Web')\" class=\"globe icon\"></i>\n                  {{getCurrentProject.contact_name}}\n               </h1>\n                <h3>{{getCurrentProject.contact_company}}</h3>\n\n                <div v-if=\"getCurrentProject.role_type\">\n                <label  class=\"highlight\">Role</label>\n                <p>{{getCurrentProject.role_type}}</p>\n              </div>\n\n                <label class=\"highlight\">Tools</label>\n                <p>{{getCurrentProject.tools}}</p>\n\n                <label class=\"highlight\">Description</label>\n                <p>{{getCurrentProject.description}}</p>\n\n                <div v-if=\"getCurrentProject.business_problem\">\n                  <label class=\"highlight\">Business Problem</label>\n                  <p>{{getCurrentProject.business_problem}}</p>\n                </div>\n\n                <div v-if=\"getCurrentProject.paid_opportunities\">\n                  <label class=\"highlight\">Paid Opportunities</label>\n                  <p>{{getCurrentProject.paid_opportunities}}</p>\n                </div>\n\n                <div class=\"spacer\">\n\n                </div>\n\n                <div>\n                <label class=\"highlight\">Contact</label>\n                <p v-if=\"getCurrentProject.contact_website\"><i class=\"linkify icon\"></i>{{getCurrentProject.contact_website}}</p>\n              